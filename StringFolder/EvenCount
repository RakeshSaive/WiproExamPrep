class HelloWorld {
    public static void main(String[] args) {
       
       String str="I lunawath venkateshwarlu";
       String[] w=str.split(" ");
       int c=0;
       for(int i=0;i<w.length;i++){
           
           for(int j=0;j<w[i].length();j++){
               if(j%2!=0){
                   c++;
               }
           }
       }
       System.out.println(c);
    }
}
